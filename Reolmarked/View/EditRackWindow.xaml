<Window x:Class="Reolmarked.View.EditRackWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Reolmarked.View"
        mc:Ignorable="d"
        Title="EditRackWindow" Height="450" Width="800">
    <Grid>
        <TextBox Text="{Binding RackToEdit.RackNumber}" IsEnabled="False" Margin="168,105,556,266"></TextBox>
        <TextBox Text="{Binding RackToEdit.RackPrice, Mode=TwoWay}" HorizontalAlignment="Left" Margin="168,206,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Redigere reol" Command="{Binding EditRackCommand}" Margin="168,259,512,109"></Button>

    </Grid>
</Window>
